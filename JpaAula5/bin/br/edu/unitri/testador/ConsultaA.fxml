<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="br.edu.unitri.testador.ConsultaAFX">
	<top>
		<TabPane fx:id="tabTela" prefHeight="404.0" prefWidth="500.0"
			tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER"
			xmlns:fx="http://javafx.com/fxml">
			<tabs>
				<Tab fx:id="tabConsulta" text="Consulta">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0">
							<children>
								<TextField fx:id="txtBuscar" layoutX="185.0" layoutY="10.0"
									prefHeight="25.0" prefWidth="290.0" />
								<Button fx:id="btnBuscar" layoutX="497.0" layoutY="10.0"
									mnemonicParsing="false" onAction="#btnBuscarClick" prefHeight="25.0"
									prefWidth="89.0" text="Buscar" />
								<TableView fx:id="tbDados" layoutX="6.0" layoutY="43.0"
									prefHeight="322.0" prefWidth="589.0" xmlns:fx="http://javafx.com/fxml" />
								<RadioButton fx:id="rbNomeDep" layoutX="14.0"
									layoutY="14.0" mnemonicParsing="false" prefHeight="17.0"
									prefWidth="171.0" selected="true" text="Por Nome Departamento">
									<toggleGroup>
										<ToggleGroup fx:id="buscarPor" />
									</toggleGroup>
								</RadioButton>
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</top>
</BorderPane>
