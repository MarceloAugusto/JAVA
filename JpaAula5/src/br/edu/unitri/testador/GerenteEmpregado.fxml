<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="br.edu.unitri.testador.GerenteEmpregadoFX">
	<top>
		<TabPane fx:id="tabTela" prefHeight="404.0" prefWidth="500.0"
			tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER"
			xmlns:fx="http://javafx.com/fxml">
			<tabs>
				<Tab fx:id="tabConsulta" text="Consulta">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0">
							<children>
								<TextField fx:id="txtBuscar" layoutX="237.0" layoutY="25.0"
									prefHeight="25.0" prefWidth="250.0" />
								<Button fx:id="btnBuscar" layoutX="497.0" layoutY="25.0"
									mnemonicParsing="false" onAction="#btnBuscarClick" prefHeight="25.0"
									prefWidth="89.0" text="Buscar" />
								<TableView fx:id="tbGerenteEmpregados" layoutX="6.0"
									layoutY="59.0" prefHeight="306.0" prefWidth="589.0"
									xmlns:fx="http://javafx.com/fxml" />
								<RadioButton fx:id="rbGerente" layoutX="23.0"
									layoutY="14.0" mnemonicParsing="false" prefHeight="17.0"
									prefWidth="168.0" text="Nome Gerente">
									<toggleGroup>
										<ToggleGroup fx:id="buscarPor" />
									</toggleGroup>
								</RadioButton>
								<RadioButton fx:id="rbEmpregado" layoutX="23.0"
									layoutY="37.0" mnemonicParsing="false" prefHeight="17.0"
									prefWidth="166.0" text="Nome Empregado" toggleGroup="$buscarPor"
									xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" />
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab fx:id="tabCadastro" text="Cadastro">
					<content>
						<Pane prefHeight="200.0" prefWidth="200.0">
							<children>
								<GridPane layoutX="93.0" layoutY="74.0" prefHeight="119.0"
									prefWidth="379.0" xmlns:fx="http://javafx.com/fxml">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="185.0" minWidth="10.0" prefWidth="98.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="293.0" minWidth="10.0" prefWidth="281.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints maxHeight="73.0" minHeight="10.0"
											prefHeight="60.0" vgrow="SOMETIMES" />
										<RowConstraints maxHeight="111.0" minHeight="10.0"
											prefHeight="59.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label text="Gerente">
										</Label>
										<Label text="Empregado" GridPane.rowIndex="1">
										</Label>
										<Pane prefHeight="200.0" prefWidth="200.0"
											GridPane.rowIndex="1" />
										<ComboBox fx:id="cbGerente" prefHeight="25.0"
											prefWidth="276.0" GridPane.columnIndex="1"
											xmlns:fx="http://javafx.com/fxml" />
										<ComboBox fx:id="cbEmpregado" prefHeight="25.0"
											prefWidth="280.0" GridPane.columnIndex="1" GridPane.rowIndex="1"
											xmlns:fx="http://javafx.com/fxml" />
									</children>
								</GridPane>
								<Button fx:id="btnIncluir" layoutX="311.0" layoutY="263.0"
									mnemonicParsing="false" onAction="#btnIncluirClick" prefHeight="25.0"
									prefWidth="90.0" text="Salvar" />
								<Button fx:id="btnNovo" layoutX="210.0" layoutY="263.0"
									mnemonicParsing="false" onAction="#btnNovoClick" prefHeight="25.0"
									prefWidth="90.0" text="Novo" xmlns="http://javafx.com/javafx/8"
									xmlns:fx="http://javafx.com/fxml/1" />
							</children>
						</Pane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</top>
</BorderPane>
